<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<Title>The utility tool that disassembles and analyzes 16bit programs</Title>
	<PackageProjectUrl>https://github.com/rajko-horvat/Disassembler</PackageProjectUrl>
    <RepositoryUrl>https://github.com/rajko-horvat/Disassembler</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
	<Authors>Rajko Horvat</Authors>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="..\LICENSE" />
    <None Remove="..\README.md" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="..\LICENSE">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="..\README.md">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
